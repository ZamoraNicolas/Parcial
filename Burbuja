
#include <iostream>

using namespace std;

int main()
{
   int numeros[]={2,1,3,4,6,90,8,7,0,9,10,11,31,42,65,76,32,54,12,65,12,43,33,87,98,46,83,54,87,45,23};
    
    int i,j,aux;
    
    for(i=0;i<29;i++){
        
        for(j=0;j<29;j++){
            
            if(numeros[j]>numeros[j+1]){
            aux = numeros[j];
            numeros[j] = numeros[j+1];
            numeros[j+1]=aux;
        
            
        }
    }    
}
    
    for(j=0;j<29;j++){
        cout<<numeros[j]<<" ";
    }
    
    return 0;
}


